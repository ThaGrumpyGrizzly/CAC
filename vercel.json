{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://cac-production.up.railway.app/api/$1"
    }
  ],
  "functions": {
    "frontend/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
} 